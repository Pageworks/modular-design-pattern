{% extends '_layouts/base' %}

{% if not craft.app.request.isLivePreview %}
    {% do eagerLoadElements(entry, [
        'cc',
        'cc.sectionSetings:backgroundImage',
        'cc.image:image',
        'cc.buttons:buttons',
        'cc.gallery:slides',
        'cc.accordion',
        'cc.accordion:rows',
        'cc.list',
        'cc.list:listItems',
        'cc.callToActionCards',
        'cc.callToActionCards:selectedCards',
        'cc.table',
        'cc.table:tableContent'
    ]) %}
{% endif %}

{% block content %}
    {% if currentUser and currentUser.admin and entry.bool1|length %}
        {% include '_includes/components/complex-content-panel' %}
    {% endif %}
    {% include '_complex-content/index' %}
{% endblock %}
