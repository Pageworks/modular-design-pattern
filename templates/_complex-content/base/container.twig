<div class="js-complex-content-container c-complex-content_container {{ block.paddingType }}{{ block.paddingSize }} o-container{{ block.width }}">
    {% switch columnLayout %}
        {% case 2 %}
            <div class="o-layout -split-50/50 {{ block.columnGutter }} {{ block.verticalAlign }}">
        {% case 3 %}
            <div class="o-layout -split-33/33/33 {{ block.columnGutter }} {{ block.verticalAlign }}">
        {% case 4 %}
            <div class="o-layout -split-33/66 {{ block.columnGutter }} {{ block.verticalAlign }}">
        {% case 5 %}
            <div class="o-layout -split-66/33 {{ block.columnGutter }} {{ block.verticalAlign }}">
        {% default %}
            <div class="o-layout {{ block.columnGutter }} {{ block.verticalAlign }}">
    {% endswitch %}
